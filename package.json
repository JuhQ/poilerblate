{
  "name": "poilerblate",
  "version": "0.4.0",
  "description": "React boilerplate",
  "main": "index.js",
  "repository": "github:skope/poilerblate",
  "bugs": {
    "url": "https://github.com/skope/poilerblate/issues"
  },
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "build": "webpack -p --config webpack.config.js",
    "test": "jest -u"
  },
  "author": "Sami Kein√§nen <skope@iki.fi>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.0.1",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.5",
    "precss": "^2.0.0",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.4.5"
  },
  "babel": {
    "presets": [
      "env",
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    }
  },
  "postcss": {
    "plugins": {
      "precss": {},
      "autoprefixer": {}
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(s(a|c)ss)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "modulePaths": [
      "src"
    ]
  },
  "dependencies": {
    "history": "^4.6.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
