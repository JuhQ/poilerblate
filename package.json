{
  "name": "boilerplate",
  "version": "0.1.0",
  "description": "Web page boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline",
    "build": "webpack -p --config webpack.config.js",
    "test": "jest"
  },
  "author": "Sami Kein√§nen <skope@iki.fi>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-css-modules": "^1.1.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "react-test-renderer": "^15.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.5.0",
    "webpack-dev-server": "^2.4.5"
  },
  "babel": {
    "presets": [
      "env",
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-react-jsx",
      "transform-object-rest-spread"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "browser": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "no-console": 0
    }
  },
  "postcss": {
    "plugins": {
      "precss": {},
      "autoprefixer": {}
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(s(a|c)ss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "dependencies": {
    "history": "^4.6.1",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0"
  }
}
